<text id="1198" date="2011-01-17" length="medium" lengthw="427" duration="medium" durations="213.0" speed="slow" speedwm="120.3" delivery="read" topic="Politics" topicspec="European consensus on humanitarian aid" type="English interpretations" comments="NA">
