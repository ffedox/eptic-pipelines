<text id="1240" date="2011-01-19" length="short" lengthw="235" duration="short" durations="98.0" speed="medium" speedwm="143.9" delivery="mixed" topic="Politics" topicspec="EU Libya Framework Agreement" type="English interpretations" comments="NA">
