<text id="932" date="2011-02-15" length="long" lengthw="99" duration="short" durations="67.0" speed="slow" speedwm="88.7" delivery="impromptu" topic="Economics and Finance" topicspec="Explanations of vote" type="Italian interpretations" comments="NA">
