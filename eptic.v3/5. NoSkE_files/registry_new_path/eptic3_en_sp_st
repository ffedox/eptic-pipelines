ATTRIBUTE   lc {
        LABEL   "word (lowercase)"
        DYNAMIC  utf8lowercase
        DYNLIB   internal
        ARG1     "C"
        FUNTYPE  s
        FROMATTR word
        TYPE     index
        TRANSQUERY      yes
}

